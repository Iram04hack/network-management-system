# Configuration développement pour AI Assistant Frontend
# Intégration avec backend validé (score 8.9/10)

# API Configuration
REACT_APP_API_URL=https://localhost:8000
REACT_APP_WS_URL=wss://localhost:8000

# Authentication (Backend validé)
REACT_APP_API_USERNAME=test_user
REACT_APP_API_PASSWORD=test_password

# Feature Flags pour migration progressive
REACT_APP_NEW_CHATBOT=true
REACT_APP_WEBSOCKET_ENABLED=true
REACT_APP_DOCUMENT_UPLOAD=true
REACT_APP_COMMAND_EXECUTION=true
REACT_APP_NETWORK_ANALYSIS=true

# Debug et Monitoring
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Timeouts et Limites
REACT_APP_API_TIMEOUT=10000
REACT_APP_UPLOAD_TIMEOUT=60000
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_MAX_RETRY_ATTEMPTS=3

# UI Configuration
REACT_APP_DEFAULT_PAGE_SIZE=20
REACT_APP_SEARCH_DEBOUNCE_MS=300
REACT_APP_CHATBOT_POSITION=bottom-right
REACT_APP_THEME=light

# Validation des données réelles (contrainte 95.65%)
REACT_APP_ENFORCE_REAL_DATA=true
REACT_APP_REAL_DATA_THRESHOLD=95.65
