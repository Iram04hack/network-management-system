          schema:
            type: object
            properties:
              policy_id:
                description: ID de la politique
                type: integer
              allocations:
                type: array
                items:
                  type: object
                  properties:
                    class_name:
                      description: Nom de la classe
                      type: string
                    bandwidth_percent:
                      description: Pourcentage alloué
                      type: number
                    guaranteed_rate_kbps:
                      description: Débit garanti (kbps)
