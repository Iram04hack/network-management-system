            type: object
            properties:
              policy_id:
                description: ID de la politique
                type: integer
              policy_name:
                description: Nom de la politique
                type: string
              bandwidth_limit:
                description: Limite de bande passante (kbps)
                type: integer
              traffic_classes:
                description: Classes de trafic configurées
                type: array
                items:
                  type: object
              traffic_data:
                description: Données de trafic temps réel
                type: object
                properties:
                  current_usage:
                    description: Utilisation actuelle
                    type: number
                  peak_usage:
                    description: Pic d'utilisation
                    type: number
