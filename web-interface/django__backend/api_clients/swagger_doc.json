                type: boolean
              message:
                description: Message de validation
                type: string
        '400':
          description: Politique invalide
          schema:
            type: object
            properties:
              valid:
                description: Statut de validation (false)
                type: boolean
              detail:
                description: Description de l'erreur
                type: string
