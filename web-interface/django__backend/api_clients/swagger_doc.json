        title: Largeur
        description: Largeur du widget en unités de grille
        type: integer
        maximum: 32767
        minimum: 0
      height:
        title: Hauteur
        description: Hauteur du widget en unités de grille
        type: integer
        maximum: 32767
        minimum: 0
